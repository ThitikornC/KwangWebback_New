generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
// ผลงาน
model Solution {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  projectName      String
  description      String
  linkVideoPreview String
  date             DateTime @default(now())
  linkPresentSheet String
  type             String
  tag              String
}
// สินค้า
model Products_cctv {
  Products_id   String   @id @default(auto()) @map("_id") @db.ObjectId
  ProductBrand  String
  ProductCode   String
  type          String
  price         Int
  salePercent   Int
  dateSell      DateTime @default(now())
  Keyfeature    String
  DataSheetLink String
  Color         Json
  len           Json
  image         String
  images        String[]
  tag           Json
}

